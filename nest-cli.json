{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "builder": "swc",
    "typeCheck": true,
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {
          "classValidatorShim": true,
          "introspectComments": false,
          "dtoFileNameSuffix": [".dto.ts", ".response.ts", ".model.ts"]
        }
      }
    ]
  }
}
