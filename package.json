{
  "name": "image",
  "version": "1.0.0",
  "description": "",
  "author": "Seweryn Swietlik",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --path tsconfig.build.json",
    "start": "nest start --path tsconfig.json",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "tsc": "node node_modules/typescript/bin/tsc --project tsconfig.build.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "format": "yarn prettier -w ./src",
    "format:check": "yarn prettier --check ./src",
    "precommit": "yarn tsc && yarn test && yarn code-check",
    "code-check": "yarn prettier -w src && yarn lint --fix && git add .",
    "upd": "yarn upgrade-interactive --latest",
    "migration:create": "ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "migration:generate": "ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/lib/config/typeorm-migrations.datasource",
    "migration:run": "ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/lib/config/typeorm-migrations.datasource",
    "migration:run:prod": "node ./node_modules/typeorm/cli.js migration:run -d ./dist/lib/config/typeorm-migrations.datasource.js",
    "migration:revert": "ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/lib/config/typeorm-migrations.datasource"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.932.0",
    "@aws-sdk/s3-request-presigner": "3.932.0",
    "@nestjs/common": "11.1.9",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.9",
    "@nestjs/platform-express": "11.1.9",
    "@nestjs/swagger": "11.2.1",
    "@nestjs/terminus": "11.0.0",
    "@nestjs/throttler": "6.4.0",
    "@nestjs/typeorm": "11.0.0",
    "@types/sharp": "0.32.0",
    "body-parser": "2.2.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "date-fns": "4.1.0",
    "express-basic-auth": "1.2.1",
    "helmet": "8.1.0",
    "ioredis": "5.8.2",
    "nestjs-throttler-storage-redis": "0.5.1",
    "pg": "8.16.3",
    "ramda": "0.32.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.1.0",
    "rxjs": "7.8.2",
    "sharp": "0.34.5",
    "typeorm": "0.3.27"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@nestjs/cli": "11.0.10",
    "@nestjs/schematics": "11.0.9",
    "@nestjs/testing": "11.1.9",
    "@swc/cli": "0.7.9",
    "@swc/core": "1.15.2",
    "@types/express": "5.0.5",
    "@types/jest": "30.0.0",
    "@types/multer": "2.0.0",
    "@types/node": "24.10.1",
    "@types/ramda": "0.31.1",
    "@types/supertest": "6.0.3",
    "eslint": "9.36.0",
    "eslint-config-codemask": "2.2.1",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "prettier": "3.6.2",
    "string-width": "4.2.3",
    "strip-ansi": "6.0.1",
    "supertest": "7.1.4",
    "ts-jest": "29.4.5",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "wrap-ansi": "7.0.0"
  },
  "resolutions": {
    "string-width": "4.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testEnvironment": "node"
  }
}
